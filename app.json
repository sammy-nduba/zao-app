// app.json
{
  "expo": {
    "name": "zao",
    "slug": "zao",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon1.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash1.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*",
      "assets/**/*",
      "src/assets/**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon1.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.sammynduba.zao"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "685e8fd8-194d-4295-9f3f-372f9f419bf1"
      }
    },
    "owner": "nduba"
  },
  "packagerOpts": {
    "config": "metro.config.js",
    "sourceExts": ["js", "jsx", "ts", "tsx", "cjs"]
  }
}